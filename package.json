{
  "name": "png-sequencer",
  "private": false,
  "version": "0.1.0",
  "type": "module",
  "description": "Highly controllable PNG sequence player for React apps.",
  "main": "./dist/png-sequencer.cjs",
  "module": "./dist/png-sequencer.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/png-sequencer.mjs",
      "require": "./dist/png-sequencer.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc --project tsconfig.lib.json && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "keywords": [
    "react",
    "png",
    "sequencer",
    "sprites",
    "animation"
  ],
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  },
  "license": "MIT"
}
